{
  "rules": {
    "arrow-parens": false,
    //"max-line-length": {
      //"options": [ 220 ]
    //},
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false,
    "no-console": {
      "options": [
        "debug",
        "info",
        "log",
        "time",
        "timeEnd",
        "trace"
      ]
    },
    "no-trailing-whitespace": true,
    "quotemark": [ true, "single", "jsx-double" ],
    "radix": true,
    "semicolon": true,
    "whitespace": false
  },
  "jsRules": {
    //"max-line-length": {
      //"options": [ 220 ]
    //}
  }
}