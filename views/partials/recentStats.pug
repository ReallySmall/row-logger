section.col-md-9
  h2 Recent sessions
  ul.list-group
    each element in userData
      li.list-group-item.col-md-6
        h3 #{dateTimeHelpers.formatDateHumanFriendly(element.createdAt)}
        table.table.table-striped.table-dark
          tr
            td Distance
            td #{rowingDataHelpers.timesToKm(element.times, 4.8) + 'km'}
          tr
            td Duration
            td #{dateTimeHelpers.timesToDuration(element.times)}
        p
          a.btn(href='#') More...
  p
    a.btn.btn-primary(href='#') View all